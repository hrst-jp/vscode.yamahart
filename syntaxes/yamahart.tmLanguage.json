{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "YAMAHA RT",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"match": "^account threshold(\\spp)?",
			"name": "entity.name.function"
		},
		{
			"match": "^(administrator|login) radius auth",
			"name": "entity.name.function"
		},
		{
			"match": "^(administrator|login)(\\spassword)?(\\sencrypted)?",
			"name": "entity.name.function"
		},
		{
			"match": "^alarm (batch|entire|http revision-up|http upload|lua|mobile|sd|startup|usbhost)",
			"name": "entity.name.function"
		},
		{
			"match": "^alias",
			"name": "entity.name.function"
		},
		{
			"include": "#tunnel"
		},
		{
			"include": "#ppp"
		},
		{
			"include": "#pp"
		},
		{
			"include": "#ipsec"
		},
		{
			"include": "#dhcp"
		},
		{
			"include": "#dns"
		},
		{
			"include": "#nat"
		},
		{
			"include": "#ipfilter"
		},
		{
			"include": "#ip"
		},
		{
			"include": "#syslog"
		},
		{
			"include": "#snmp"
		},
		{
			"match": "\\b(myname|name|text) (\\S+)(\\s\\S+)?",
			"captures": {
				"2": {
					"name": "string"
				},
				"3": {
					"name": "string"
				}
			}
		},
		{
			"match": "(pp|ipsec ike|l2tp|wan) always-on (on|off)",
			"name": "keyword",
			"captures": {
				"2": {
					"name": "keyword.control"
				}
			}
		},
		{
			"match": "gateway",
			"name": "keyword"
		},
		{
			"match": "ntpdate \\b(\\S+)\\b",
			"name": "keyword",
			"captures": {
				"1": {
					"name": "string"
				}
			}
		},
		{
			"include": "#commands"
		},
		{
			"include": "#address"
		},
		{
			"include": "#keyword"
		},
		{
			"include": "#interface"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"match": "#.*",
					"name": "comment.line"
				},
				{
					"match": "(tunnel|pp) name .*",
					"name": "comment"
				}
			]
		},
		"keyword": {
			"patterns": [
				{
					"match": "\\b(backup|select|enable|disable)\\b",
					"name": "keyword.control"
				},
				{
					"match": "\\b(auto|on|off|in|out|only|none|hide|weight|dynamic|static)\\b",
					"name": "keyword.control"
				},
				{
					"match": "\\b(icmp|tcp|udp|gre|esp|ah|icmp6)\\b",
					"name": "variable.language"
				},
				{
					"match": "\\b(ftp|ftpdata|telnet|smtp|domain|gopher|finger|www|pop3|sunrpc|ident|ntp|nntp|snmp|syslog|printer|talk|route|uucp|submission)\\b",
					"name": "variable.language"
				},
				{
					"match": "\\b(icmp-error|icmp-info|tcpsyn|tcpfin|tcprst|established)\\b",
					"name": "variable.language"
				},
				{
					"match": "\\b(([0-1][0-9]|2[0-3]):[0-5][0-9])\\b",
					"name": "constant.numeric",
					"comment": "時間"
				},
				{
					"match": "\\b\\d+\\b",
					"name": "constant.numeric"
				}
			]
		},
		"commands": {
			"patterns": [
				{
					"match": "^\\b(yno|wol|wins|wan|vlan|user|usbhost|url|upnp|tunnel|tftp|terminate|telnetd|tcp)\\b",
					"name": "entity.name.function"
				},
				{
					"match": "^\\b(system|switch|sshd|sntpd|snmpv3|snmpv2c|snmp|sip|schedule|sftpd|security|rtfs|rotate|rip)\\b",
					"name": "entity.name.function"
				},
				{
					"match": "^\\b(remote|radius|queue|qac-tm|provider|pri|pptp|pppoe|ppp|pp|pki|ospf|operation)\\b",
					"name": "entity.name.function"
				},
				{
					"match": "^\\b(ntp|ngn|netvolante-dns|mobile|mail|login|leased|lan-map|lan|l2tp|jate)\\b",
					"name": "entity.name.function"
				},
				{
					"match": "^\\b(isdn|ipsec|ipip|ip|httpd|http|heartbeat2|heartbeat|fr)\\b",
					"name": "entity.name.function"
				},
				{
					"match": "^\\b(external-memory|execute|ethernet|dns|diagnose|dhcp|dashboardaccumulate)\\b",
					"name": "entity.name.function"
				},
				{
					"match": "^\\b(console|cloud|bridge|bgp|auth|ap|alarm|account|administrator)\\b",
					"name": "entity.name.function"
				}
			]
		},
		"address": {
			"patterns": [
				{
					"match": "\\b(([1-9]?[0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([1-9]?[0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])(/(([1-2]?[0-9])|3[0-2]))?\\b",
					"name": "string",
					"comment": "IPv4 Address"
				},
				{
					"match": "\\b([0-9a-f]{0,4}:){7}[0-9a-f]{0,4}(%\\S+|(/([1-9]?[0-9]|1[0-9]{2}|12[0-8])))?\\b",
					"name": "string",
					"comment": "IPv6 Address省略なし"
				},
				{
					"match": "\\b([0-9a-f]{0,4}:){3}(:|[0-9a-f]{0,4})?(:[0-9a-f]{0,4}){0,4}(%\\S+|(/([1-9]?[0-9]|1[0-9]{2}|12[0-8])))?\\b",
					"name": "string",
					"comment": "IPv6 Address省略あり48bitマスク以上"
				},
				{
					"match": "\\b(([0-9a-fA-F]{2}(:|-)){5}[0-9a-fA-F]{2})\\b",
					"name": "string",
					"comment": "MACアドレス"
				}
			]
		},
		"interface": {
			"patterns": [
				{
					"match": "\\b(lan\\d(\\.\\d|/\\d)?)\\b",
					"name": "variable.language"
				},
				{
					"match": "\\b(bri[1-2](\\.\\d)?)\\b",
					"name": "variable.language"
				},
				{
					"match": "\\b(vlan\\d|pri1|bridge1|null|tunnel)\\b",
					"name": "variable.language"
				},
				{
					"match": "\\b(loopback[1-9])\\b",
					"name": "variable.language"
				},
				{
					"match": "(pp|tunnel) (enable|disable|select)",
					"name": "storage.type",
					"captures": {
						"2": {
							"name": "keyword.control"
						}
					}
				}
			]
		},
		"ipfilter": {
			"patterns": [
				{
					"match": "^(ip|ipv6) filter directed-broadcast (filter)",
					"name": "entity.name.function",
					"captures": {
						"2": {
							"name": "keyword.control"
						}
					}
				},
				{
					"match": "(ip|ipv6) filter (directed-broadcast|source-route)",
					"name": "entity.name.function"
				},
				{
					"match": "(ip|ipv6) filter (\\d+) (\\S+) ",
					"name": "entity.name.function",
					"captures": {
						"2": {
							"name": "constant.numeric"
						},
						"3": {
							"name": "keyword.control"
						}
					}
				},
				{
					"match": "(ip|ipv6) filter dynamic (\\d+)",
					"name": "entity.name.function",
					"captures": {
						"2": {
							"name": "constant.numeric"
						}
					}
				}
			]
		},
		"ip": {
			"patterns": [
				{
					"match": "(ip|ipv6) (\\S*) address",
					"name": "entity.name.function",
					"captures": {
						"2": {
							"name": "variable.language"
						}
					}
				},
				{
					"match": "(ip|ipv6) (\\S+) (mtu|nat descriptor)",
					"name": "entity.name.function",
					"captures": {
						"2": {
							"name": "variable.language"
						}
					}
				},
				{
					"begin": "^(ip|ipv6) route",
					"end": "\\n",
					"patterns": [
						{
							"include": "#address"
						},
						{
							"match": "\\b(\\d+)\\b",
							"name": "constant.numeric"
						},
						{
							"match": "(default|pp|dhcp|tunnel)",
							"name": "string"
						},
						{
							"match": "gateway",
							"name": "keyword"
						},
						{
							"match": "(filter|metric|hide|weight|keepalive)",
							"name": "keyword.control"
						}
					],
					"name": "entity.name.function"
				},
				{
					"match": "\\S* (\\S*) tcp mss limit",
					"name": "entity.name.function",
					"captures": {
						"1": {
							"name": "keyword"
						}
					}
				},
				{
					"match": "(ip|ipv6) (\\S*) secure filter",
					"name": "entity.name.function",
					"captures": {
						"2": {
							"name": "variable.language"
						}
					}
				},
				{
					"match": "(ip|ipv6) (\\S*) intrusion detection",
					"name": "entity.name.function",
					"captures": {
						"2": {
							"name": "variable.language"
						}
					}
				}
			]
		},
		"ppp": {
			"patterns": [
				{
					"match": "ppp (lcp mru|ccp type|ipcp msext|ipcp ipaddress)",
					"name": "entity.name.function"
				}
			]
		},
		"pp": {
			"patterns": [
				{
					"match": "pp auth accept (pap|chap|mschap|mschap-v2)( (pap|chap|mschap|mschap-v2))?",
					"name": "entity.name.function",
					"captures": {
						"1": {
							"name": "keyword.control"
						},
						"2": {
							"name": "keyword.control"
						}
					}
				},
				{
					"match": "pp auth (myname|username) (\\S+) (\\S+)",
					"name": "entity.name.function",
					"captures": {
						"2": {
							"name": "string"
						}
					}
				},
				{
					"match": "pppoe use",
					"name": "entity.name.function"
				}
			]
		},
		"tunnel": {
			"patterns": [
				{
					"match": "tunnel encapsulation (ipsec|ipip|pptp|l2tpv3-raw|l2tpv3|l2tp|ipudp|map-e)",
					"name": "entity.name.function",
					"captures": {
						"1": {
							"name": "keyword.control"
						}
					}
				},
				{
					"match": "tunnel backup",
					"name": "entity.name.function"
				}
			]
		},
		"snmp": {
			"patterns": [
				{
					"match": "^snmp community (read-only|read-write) (\\S+)",
					"name": "entity.name.function",
					"captures": {
						"2": {
							"name": "string"
						}
					}
				},
				{
					"match": "^snmp sys(contact|name|location) (\\S+)",
					"name": "entity.name.function",
					"captures": {
						"2": {
							"name": "string"
						}
					}
				},
				{
					"match": "^snmp (display|host|ifindex|local|trap)",
					"name": "entity.name.function"
				},
				{
					"match": "^snmp (yrifppdisplayatmib2|yrifswitchdisplayatmib2|yriftunneldisplayatmib2)",
					"name": "entity.name.function"
				}
			]
		},
		"syslog": {
			"patterns": [
				{
					"match": "^syslog (debug|info|notice|execute command)",
					"name": "entity.name.function"
				},
				{
					"match": "^syslog (srcport|host|local address)",
					"name": "entity.name.function"
				},
				{
					"match": "\\S* (syslog)",
					"name": "entity.name.function"
				}
			]
		},
		"ipsec": {
			"patterns": [
				{
					"match": "ipsec ike pre-shared-key (\\d+) (text){0,1} (\\S+)",
					"name": "entity.name.function",
					"captures": {
						"1": {
							"name": "constant.numeric"
						},
						"2": {
							"name": "constant.language"
						},
						"3": {
							"name": "string"
						}
					}
				},
				{
					"match": "ipsec sa policy (\\d+) (\\d+) esp (3des-cbc|des-cbc|aes-cbc|aes256-cbc)+ (md5-hmac|sha-hmac|sha256-hmac)+",
					"name": "entity.name.function",
					"captures": {
						"1": {
							"name": "constant.numeric"
						},
						"2": {
							"name": "constant.numeric"
						},
						"3": {
							"name": "string"
						},
						"4": {
							"name": "string"
						}
					}
				},
				{
					"match": "ipsec ike keepalive use (\\d+) (on|off|auto) (heartbeat|dpd|rfc4306)+ (\\d+) (\\d+)",
					"name": "entity.name.function",
					"captures": {
						"1": {
							"name": "constant.numeric"
						},
						"2": {
							"name": "keyword.control"
						},
						"4": {
							"name": "string"
						},
						"5": {
							"name": "string"
						}
					}
				},
				{
					"match": "ipsec ike always-on",
					"name": "entity.name.function"
				},
				{
					"match": "ipsec ike (local|remote) (address|id|name) (\\d+)",
					"name": "entity.name.function",
					"captures": {
						"3": {
							"name": "constant.numeric"
						}
					}
				},
				{
					"match": "ipsec tunnel (\\d+)",
					"name": "entity.name.function",
					"captures": {
						"1": {
							"name": "constant.numeric"
						}
					}
				}
			]
		},
		"dhcp": {
			"patterns": [
				{
					"match": "dhcp service (server|relay)",
					"name": "entity.name.function",
					"captures": {
						"1": {
							"name": "keyword.control"
						}
					}
				},
				{
					"begin": "dhcp scope option",
					"end": "\\n",
					"patterns": [
						{
							"include": "#address"
						},
						{
							"match": "\\b(\\d+)\\b",
							"name": "constant.numeric"
						},
						{
							"match": "\\b(router|dns|hostname|domain|wins_server)(=)([0-9a-zA-Z.]+)",
							"captures": {
								"1": {
									"name": "keyword"
								},
								"2": {
									"name": "keyword.operator"
								},
								"3": {
									"name": "string"
								}
							}
						}
					],
					"name": "entity.name.function"
				},
				{
					"match": "^dhcp scope \\b(except|gateway|expire|maxexpire)\\b",
					"name": "keyword.control"
				},
				{
					"match": "infinity",
					"name": "constant.character"
				},
				{
					"match": "^dhcp (client|convert|duplicate|manual|relay|scope|server|service)",
					"name": "entity.name.function"
				}
			]
		},
		"dns": {
			"patterns": [
				{
					"match": "dns (cache max entry|cache use|domain|host|notice order|private address spoof|server|service|srcport|static)",
					"name": "support.function"
				}
			]
		},
		"nat": {
			"patterns": [
				{
					"match": "nat descriptor address (outer|inner) (\\d+) (\\S+)",
					"name": "entity.name.function",
					"captures": {
						"1": {
							"name": "keyword"
						},
						"2": {
							"name": "constant.numeric"
						},
						"3": {
							"name": "string"
						}
					}
				},
				{
					"match": "nat descriptor (ftp port|log|sip|timer|type) (\\d+) (\\S+)",
					"name": "entity.name.function",
					"captures": {
						"2": {
							"name": "constant.numeric"
						},
						"3": {
							"name": "keyword.control"
						}
					}
				},
				{
					"match": "nat descriptor static (\\d+) (\\d+) (\\S+)",
					"name": "entity.name.function",
					"captures": {
						"1": {
							"name": "constant.numeric"
						},
						"2": {
							"name": "constant.numeric"
						},
						"3": {
							"name": "string"
						}
					}
				},
				{
					"match": "^nat descriptor (masquerade|backward-compatibility)",
					"name": "entity.name.function"
				}
			]
		}
	},
	"scopeName": "text.yamahart"
}
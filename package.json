{
    "name": "yamaha-rt",
    "displayName": "YAMAHA RT",
    "description": "YAMAHA RT config",
    "version": "1.0.4",
    "publisher": "hrst",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "yamahart",
                "aliases": [
                    "YAMAHA RT",
                    "yamahart"
                ],
                "extensions": [
                    ""
                ],
                "configuration": "./language-configuration.json",
                "firstLine": "^#.*YAMAHA RT"
            }
        ],
        "grammars": [
            {
                "language": "yamahart",
                "scopeName": "yamahart",
                "path": "./syntaxes/yamahart.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "yamahart",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}
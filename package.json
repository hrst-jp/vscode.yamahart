{
  "name": "yamaha-rt",
  "displayName": "YAMAHA Router series",
  "description": "Highlight the configuration file of YAMAHA router product.",
  "version": "2.0.2",
  "publisher": "hrst",
  "icon": "images/rtx1210_icon.png",
  "engines": {
    "vscode": "^1.16.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "yamahart",
        "aliases": [
          "YAMAHA RTX",
          "YAMAHA NVR"
        ],
        "firstLine": "^# (# )?(RT|NVR)\\S+ Rev\\.",
        "extensions": [
          ".rtx",
          ".nvr"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "yamahart",
        "scopeName": "text.yamahart",
        "path": "./syntaxes/yamahart.tmLanguage_v2.json"
      }
    ],
    "snippets": [
      {
        "language": "yamahart",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hrst-jp/vscode.yamahart.git"
  }
}